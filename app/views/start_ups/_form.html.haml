= form_for(@start_up) do |f|
  - if @start_up.errors.any?
    #error_explanation
      %h2
        = pluralize(@start_up.errors.count, "error")
        prohibited this start_up from being saved:
      %ul
        - @start_up.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :name
    %br/
    = f.text_field :name
  .field
    = f.label :description
    %br/
    = f.text_field :description
  .actions
    = f.submit
